#include "derivative.h" /* include peripheral declarations */



int main(void){
	SIM_SCGC5 = (1<<13)+1;
	
	PORTE_PCR26=(1<<8);
	
	GPIOE_PDDR|=(1<<26);
	GPIOE_PDDR|=(1<<26);
	
	LPTMR0_PSR=5;		//101 Bypass
	LPTMR0_CMR = 1000;
	LPTMR0_CSR=1;
	
	return 0;
}
